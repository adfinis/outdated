{{#if @dependencyVersions}}
  <table>
    <thead>
      <tr>
        <th>Dependency</th>
        <th>Version</th>
        <th>Release Date</th>
      </tr>
    </thead>
    <tbody>
      {{#each @dependencyVersions as |version|}}
        <DependencyDetailed @version={{version}} />
      {{/each}}

    </tbody>
  </table>
{{else}}
  <div class='none-yet' data-test-dependency-versions-none>
    <span class='text-UNDEFINED'>No dependencies yet</span>
  </div>
{{/if}}